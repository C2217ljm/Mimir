#!/bin/bash
#SBATCH --job-name="wordgenerator"  
#SBATCH --output="wordgenerator.%j.%N.out"
#SBATCH --partition=compute  
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --export=ALL  
#SBATCH --ntasks-per-core=1
#SBATCH -v -t 02:00:00

echo "distribute",$DIST
echo "unique",$UNIQUE
echo "fsize",$FSIZE
echo "offset",$OFFSET
echo "nfile",$NFILE
echo "prefix",$PREFIX
echo "outdir",$OURDIR

python wordgenerator.py $DIST $UNIQUE $FSIZE $OFFSET $NFILE $PREFIX $OUTDIR
